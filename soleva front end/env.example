# Soleva Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=https://solevaeg.com/api
VITE_APP_NAME=Soleva
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=development

# Analytics & Tracking
VITE_GOOGLE_ANALYTICS_ID=
VITE_FACEBOOK_PIXEL_ID=
VITE_TIKTOK_PIXEL_ID=
VITE_SNAPCHAT_PIXEL_ID=

# Payment Gateways
VITE_STRIPE_PUBLISHABLE_KEY=
VITE_PAYMOB_API_KEY=

# Maps & Location
VITE_GOOGLE_MAPS_API_KEY=

# Social Media
VITE_FACEBOOK_APP_ID=
VITE_GOOGLE_CLIENT_ID=

# Feature Flags
VITE_ENABLE_GUEST_CHECKOUT=true
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_WISHLIST=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_LIVE_CHAT=false

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true
VITE_CACHE_DURATION=3600

# Debug
VITE_DEBUG_MODE=false
VITE_SHOW_REDUX_DEVTOOLS=false
